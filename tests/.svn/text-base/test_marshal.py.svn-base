import unittest
import pyflam3.marshal as marshal
import string
from testing_util import print_test_name

class MarshallingTestCase(unittest.TestCase):
    @print_test_name
    def testFromString(self):
        s = string.ascii_letters
        c_str = marshal.from_string(s)
        self.assertEqual(s, c_str.value)

        s_from_c_str = c_str.value
        self.assertEquals(len(s_from_c_str), len(s))

